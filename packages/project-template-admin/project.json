{
  "targets": {
    "tsc-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec mono nx-cwd tsc --noEmit"
      }
    },
    "pre-build": {
      "executor": "nx:noop"
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec mono nx-cwd vite"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec mono nx-cwd vite build"
      }
    }
  }
}