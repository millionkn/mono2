{
  "targets": {
    "tsc-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec mono nx-cwd \"tsc --noEmit\""
      }
    },
    "pre-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec mono nx-cwd \"pnpm exec mono rollup\""
      }
    }
  }
}